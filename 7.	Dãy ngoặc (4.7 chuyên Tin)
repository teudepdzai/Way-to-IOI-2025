  #include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll nmax=10e5+5;
int n,k,a[nmax],d0=0,d1=0;
bool ktra(int a[])
{
	d0=0;d1=0;
	for(int i=1;i<=n;i++)
	{
	  if(a[i]==0)d0++;
	  else if(a[i]==1)d1++;	
	  if(d1>d0)return 0;
	}
	if(d0!=d1)return 0;
	return 1;
}
void in()
{
	if(ktra(a)==1)
	{
	for(int i=1;i<=n;i++)
	if(a[i]==0)cout<<"(";
	else cout<<")";
	cout<<endl; 
    }
}	void back(int i)
{
	for(int j=0;j<=1;j++)
	{
		a[i]=j;
		if(i==n)in();
		else back(i+1);
	}
}	
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//  freopen("TEST.INP","r",stdin);
//  freopen("TEST.OUT","w",stdout);
    cin>>n;
    n*=2;
    back(1);
    return 0;
}
