  #include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll nmax=10e5+5;
int n,k,a[nmax];
bool mark[nmax];
void in()
{
	for(int i=1;i<=n;i++)
	if(mark[i]==1)cout<<i<<" ";
	cout<<endl;    
}

void back(int i)
{
	for(int j=0;j<=1;j++)
	{
		mark[i]=j;
		if(i==n)in();
		else back(i+1);
		mark[i]=0;
	}
}
	int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//  freopen("TEST.INP","r",stdin);
//  freopen("TEST.OUT","w",stdout);
    cin>>n;
    
    back(1);
    return 0;
}
