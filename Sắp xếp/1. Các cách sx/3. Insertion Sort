#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
const ll nmax=10e4;
ll n, a[nmax];

void InsertionSort(ll n, ll a[])
{
	int j,key;
	for(int i=2;i<=n;i++)
	{
		key=a[i];
		j=i-1;
		while(j>=1 && a[j]>key)
		{
			a[j+1]=a[j];
			j--;
		}
		a[j+1]=key;
	}
}


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    //freopen("TEST.INP","r",stdin);
    //freopen("TEST.OUT","w",stdout);
    cin>>n;
    for(int i=1;i<=n;++i)cin>>a[i];
    
    for(int i=1;i<=n;i++)cout<<a[i]<<" ";    
    cout<<"\n\n";
    //BubbleSort(n,a);
    //SelectionSort(n,a);
    InsertionSort(n,a);
    //MergeSort(1,n,a);
    //QuickSort(a,1,n);
    for(int i=1;i<=n;i++)cout<<a[i]<<" ";    
    return 0;
}
