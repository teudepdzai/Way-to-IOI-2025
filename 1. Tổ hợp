 #include <bits/stdc++.h>
using namespace std;

typedef long long ll;
const ll nmax=10e5+5;
int n,k,a[nmax];
void in()
{
	for(int i=1;i<=k;i++)cout<<a[i]<<" ";
	cout<<endl;    
}
void back(int i)
{
	for(int j=a[i-1]+1;j<=n-k+i;j++)
	{
		a[i]=j;
		if(i==k)in();
		else back(i+1);
	}
}
	
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//  freopen("TEST.INP","r",stdin);
//  freopen("TEST.OUT","w",stdout);
    cin>>n>>k;
    a[0]=0;
    back(1);
    return 0;
}
