 #include <bits/stdc++.h>
using namespace std;

typedef long long ll;
const ll nmax=10e5+5;
int m;
string s,a[nmax],dau[2]={"+","-"};

int tinh(string s, string a[])
{
	ll d=0;
	for(int i=0;i<s.length();i++)
	{
	if(a[i]=="+")d+=(int)(s[i]-48);
	else if(a[i]=="-") d-=(int)(s[i]-48);
	}
	return d;
}
void in()
{
	if(tinh(s,a)==m)
	{
          for(int i=0;i<s.length();i++)
            cout<<a[i]<<" "<<s[i]<<" ";
	cout<<endl;
    }
}	void back( int i )
{
	for(int j=0;j<=1;j++)
	{
		a[i]=dau[j];
		if(i==s.length()-1)in();
		else back(i+1);
	}
}
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
  freopen("TEST.INP","r",stdin);
  freopen("TEST.OUT","w",stdout);
    cin>>s>>m;
    back(0);

    return 0;
}
