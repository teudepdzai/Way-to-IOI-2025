#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
const ll nmax=100005;

bool mark[nmax];
int n,m,a,b,dem=0;
vector <int> dsk[nmax];
queue <int> q;
void bfs(int u)
{
	mark[u]=1;
	q.push(u);
	while(!q.empty())
	{
		u=q.front();
		mark[u]=1;
		q.pop();
		for(int v:dsk[u])
		   if(mark[v]==0) q.push(v);
	}
	dem++;
	
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
//    freopen("TEST.INP","r",stdin);
//    freopen("TEST.OUT","w",stdout);
    cin>>n>>m;
    for(int i=1;i<=m;i++)
    {
    	cin>>a>>b;
    	dsk[a].push_back(b);
    	dsk[b].push_back(a);
	}
	memset(mark,0,nmax);
	bfs(1);
	for(int i=1;i<=n;i++)if(mark[i]==0)cout<<i<<"\n";
    return 0;
}
//https://www.spoj.com/PTIT/problems/BCDAISY/
